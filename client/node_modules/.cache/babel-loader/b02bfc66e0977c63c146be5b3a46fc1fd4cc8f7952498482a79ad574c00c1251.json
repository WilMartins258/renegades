{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport IMask from \"imask\";\nimport axios from \"axios\";\nexport default {\n  components: {},\n  name: \"AddEstabelecimento\",\n  data() {\n    return {\n      imagensSelecionadas: [],\n      imagensEstabelecimentoSelecionadas: [],\n      cnpj: \"\",\n      cep: \"\",\n      endereco: {\n        rua: \"\",\n        bairro: \"\",\n        cidade: \"\",\n        uf: \"\",\n        cnpjInput: null\n      }\n    };\n  },\n  methods: {\n    excluirMiniatura(index) {\n      this.imagensSelecionadas.splice(index, 1);\n    },\n    cancelar() {\n      this.$router.push(\"/form-wrap\");\n    },\n    limpa_formulário_cep() {\n      document.getElementById(\"rua\").value = \"\";\n      document.getElementById(\"bairro\").value = \"\";\n      document.getElementById(\"cidade\").value = \"\";\n      document.getElementById(\"uf\").value = \"\";\n    },\n    meu_callback(conteudo) {\n      if (!(\"erro\" in conteudo)) {\n        document.getElementById(\"rua\").value = conteudo.logradouro;\n        document.getElementById(\"bairro\").value = conteudo.bairro;\n        document.getElementById(\"cidade\").value = conteudo.localidade;\n        document.getElementById(\"uf\").value = conteudo.uf;\n      } else {\n        this.limpa_formulário_cep();\n        alert(\"CEP não encontrado.\");\n      }\n    },\n    pesquisarCep() {\n      var cep = this.cep.replace(/\\D/g, \"\");\n      if (cep !== \"\") {\n        var validacep = /^[0-9]{8}$/;\n        if (validacep.test(cep)) {\n          axios.get(`https://viacep.com.br/ws/${cep}/json/`).then(response => {\n            this.meu_callback(response.data);\n            this.$refs.numeroInput.focus();\n          }).catch(error => {\n            this.limpa_formulário_cep();\n            alert(\"CEP não encontrado.\");\n          });\n        } else {\n          this.limpa_formulário_cep();\n          alert(\"Formato de CEP inválido.\");\n        }\n      } else {\n        this.limpa_formulário_cep();\n      }\n    },\n    excluirEstabelecimentoMiniatura(index) {\n      this.imagensEstabelecimentoSelecionadas.splice(index, 1);\n    }\n  },\n  mounted() {\n    this.cnpjInput = this.$refs.cnpjInput; // Add ref=\"cnpjInput\" to the input element\n    const cnpjMask = IMask(this.cnpjInput, {\n      mask: \"00.000.000/0000-00\"\n    });\n    const telefoneInput = document.getElementById(\"telefone\");\n    const telefoneMask = IMask(telefoneInput, {\n      mask: \"(00) 0000-0000\"\n    });\n    const celularInput = document.getElementById(\"celular\");\n    const celularMask = IMask(celularInput, {\n      mask: \"(00) 00000-0000\"\n    });\n\n    /*Img Cardápio*/\n    const imagensInput = document.getElementById(\"imagens\");\n    imagensInput.addEventListener(\"change\", () => {\n      const files = Array.from(imagensInput.files);\n      const imagensRestantes = 3 - this.imagensSelecionadas.length;\n      const novasImagens = files.slice(0, imagensRestantes);\n      novasImagens.forEach(file => {\n        const reader = new FileReader();\n        reader.onload = () => {\n          const miniatura = {\n            src: reader.result\n          };\n          this.imagensSelecionadas.push(miniatura);\n        };\n        reader.readAsDataURL(file);\n      });\n      imagensInput.value = \"\";\n    });\n\n    /*Img Logo Estabelecimento*/\n    const estabelecimentoImagensInput = document.getElementById(\"estabelecimentoImagens\");\n    estabelecimentoImagensInput.addEventListener(\"change\", () => {\n      const files = Array.from(estabelecimentoImagensInput.files);\n      const imagensRestantes = 1 - this.imagensEstabelecimentoSelecionadas.length;\n      const novasImagens = files.slice(0, imagensRestantes);\n      novasImagens.forEach(file => {\n        const reader = new FileReader();\n        reader.onload = () => {\n          const miniatura = {\n            src: reader.result\n          };\n          this.imagensEstabelecimentoSelecionadas.push(miniatura);\n        };\n        reader.readAsDataURL(file);\n      });\n      estabelecimentoImagensInput.value = \"\";\n    });\n  }\n};","map":{"version":3,"names":["IMask","axios","components","name","data","imagensSelecionadas","imagensEstabelecimentoSelecionadas","cnpj","cep","endereco","rua","bairro","cidade","uf","cnpjInput","methods","excluirMiniatura","index","splice","cancelar","$router","push","limpa_formulário_cep","document","getElementById","value","meu_callback","conteudo","logradouro","localidade","alert","pesquisarCep","replace","validacep","test","get","then","response","$refs","numeroInput","focus","catch","error","excluirEstabelecimentoMiniatura","mounted","cnpjMask","mask","telefoneInput","telefoneMask","celularInput","celularMask","imagensInput","addEventListener","files","Array","from","imagensRestantes","length","novasImagens","slice","forEach","file","reader","FileReader","onload","miniatura","src","result","readAsDataURL","estabelecimentoImagensInput"],"sources":["C:\\Users\\Lucas\\Desktop\\TG\\renegades\\client\\src\\components\\Estabelecimento\\AddEstabelecimento.vue"],"sourcesContent":["<template>\r\n  <div id=\"form-wrap\" class=\"form\">\r\n    <div class=\"posicaoform-wrap\">\r\n         <!--Pesquisa por cep e completa os campos-->\r\n      <!--1-Endereço-->\r\n      <div v-if=\"currentStep === 1\">\r\n  <h2>Seção 1: Endereço</h2>\r\n  <div class=\"group\">\r\n    <label for=\"cep\" class=\"label\">CEP:</label><br />\r\n    <input\r\n      v-model=\"cep\"\r\n      type=\"text\"\r\n      id=\"cep\"\r\n      class=\"input\"\r\n      name=\"cep\"\r\n      @blur=\"pesquisarCep\"\r\n      required\r\n    /><br />\r\n  </div>\r\n  <div class=\"group\">\r\n    <label for=\"rua\" class=\"label\">Rua:</label><br />\r\n    <input\r\n      v-model=\"endereco.rua\"\r\n      type=\"text\"\r\n      id=\"rua\"\r\n      class=\"input\"\r\n      name=\"rua\"\r\n    /><br />\r\n  </div>\r\n  <div class=\"group\">\r\n    <label for=\"numero\" class=\"label\">Número:</label><br />\r\n    <input\r\n      ref=\"numeroInput\"\r\n      type=\"text\"\r\n      id=\"numero\"\r\n      class=\"input\"\r\n      name=\"numero\"\r\n      required\r\n    /><br />\r\n  </div>\r\n  <div class=\"group\">\r\n    <label for=\"bairro\" class=\"label\">Bairro:</label><br />\r\n    <input\r\n      v-model=\"endereco.bairro\"\r\n      type=\"text\"\r\n      id=\"bairro\"\r\n      class=\"input\"\r\n      name=\"bairro\"\r\n    /><br />\r\n  </div>\r\n  <div class=\"group\">\r\n    <label for=\"cidade\" class=\"label\">Cidade:</label><br />\r\n    <input\r\n      v-model=\"endereco.cidade\"\r\n      type=\"text\"\r\n      id=\"cidade\"\r\n      class=\"input\"\r\n      name=\"cidade\"\r\n    /><br />\r\n  </div>\r\n  <div class=\"group\">\r\n    <label for=\"uf\" class=\"label\">Estado:</label><br />\r\n    <input\r\n      v-model=\"endereco.uf\"\r\n      type=\"text\"\r\n      id=\"uf\"\r\n      class=\"input\"\r\n      name=\"uf\"\r\n    /><br />\r\n  </div>\r\n</div> <!-- Fechar \"Seção 1\" -->\r\n\r\n      <!--2-Contato-->\r\n      <div v-if=\"currentStep === 2\">\r\n  <h2>Seção 2: Contatos</h2>\r\n  <div class=\"group\">\r\n    <label for=\"telefone\" class=\"label\">Telefone:</label><br />\r\n    <input type=\"text\" v-model=\"telefone\" id=\"telefone\" class=\"input\" name=\"telefone\" required /><br />\r\n  </div>\r\n  \r\n  <div class=\"group\">\r\n    <label for=\"whatsapp\" class=\"label\">É WhatsApp?</label><br />\r\n    <input type=\"checkbox\" v-model=\"isTelefoneWhatsApp\" id=\"whatsapp\" name=\"whatsapp\" />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"celular\" class=\"label\">Celular:</label><br />\r\n    <input type=\"text\" v-model=\"celular\" id=\"celular\" class=\"input\" name=\"celular\" required /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"whatsappCelular\" class=\"label\">É WhatsApp?</label><br />\r\n    <input type=\"checkbox\" v-model=\"isCelularWhatsApp\" id=\"whatsappCelular\" name=\"whatsapp\" />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"site\" class=\"label\">Informe seu site:</label><br />\r\n    <input type=\"text\" v-model=\"site\" id=\"site\" class=\"input\" name=\"site\" /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"cardapioOnline\" class=\"label\">Cardápio Online:</label><br />\r\n    <input type=\"text\" v-model=\"cardapioOnline\" id=\"cardapioOnline\" class=\"input\" name=\"nome\" /><br />\r\n  </div>\r\n</div> <!-- Fechar \"Seção 2\" -->\r\n\r\n      <!--3-Dados do Estabelecimento-->\r\n      <div v-if=\"currentStep === 3\">\r\n  <h2>Seção 3: Dados do Estabelecimento</h2>\r\n  <div class=\"group\">\r\n    <label for=\"nome\" class=\"label\">Nome do Estabelecimento:</label><br />\r\n    <input type=\"text\" v-model=\"nomeEstabelecimento\" id=\"nome\" class=\"input\" name=\"nome\" /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"cnpj\" class=\"label\">CNPJ:</label><br />\r\n    <input v-model=\"cnpj\" type=\"text\" id=\"cnpj\" class=\"input\" name=\"cnpj\" required ref=\"cnpjInput\"\r\n/>\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"descricao\" class=\"label\">Descrição do Estabelecimento:</label><br />\r\n    <textarea v-model=\"descricaoEstabelecimento\" id=\"descricao\" class=\"input\" name=\"descricao\" rows=\"4\" maxlength=\"200\" required></textarea>\r\n    <br />\r\n    <span id=\"contador\">{{ descricaoEstabelecimento.length }}</span>/<span>200</span><br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label class=\"label\">Oferece Rodízio?</label><br />\r\n    <input type=\"radio\" v-model=\"ofereceRodizio\" value=\"Sim\" required /> Sim\r\n    <input type=\"radio\" v-model=\"ofereceRodizio\" value=\"Não\" required /> Não<br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label class=\"label\">Precisa Agendar?</label><br />\r\n    <input type=\"radio\" v-model=\"precisaAgendar\" value=\"Sim\" required /> Sim\r\n    <input type=\"radio\" v-model=\"precisaAgendar\" value=\"Não\" required /> Não<br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label class=\"label\">Tem Estacionamento?</label><br />\r\n    <input type=\"radio\" v-model=\"temEstacionamento\" value=\"Sim\" required /> Sim\r\n    <input type=\"radio\" v-model=\"temEstacionamento\" value=\"Não\" required /> Não<br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label class=\"label\">Toca Música?</label><br />\r\n    <input type=\"radio\" v-model=\"tocaMusica\" value=\"Sim\" required /> Sim\r\n    <input type=\"radio\" v-model=\"tocaMusica\" value=\"Não\" required /> Não<br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label class=\"label\">É ao Vivo?</label><br />\r\n    <input type=\"radio\" v-model=\"musicaAoVivo\" value=\"Sim\" required /> Sim\r\n    <input type=\"radio\" v-model=\"musicaAoVivo\" value=\"Não\" required /> Não<br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"generoMusica\" class=\"label\">Gênero de Música:</label><br />\r\n    <select v-model=\"generoMusica\" id=\"generoMusica\" class=\"input\" name=\"generoMusica\" required>\r\n      <option value=\"rock\">Rock</option>\r\n      <option value=\"sertanejo\">Sertanejo</option>\r\n      <option value=\"jazz\">Jazz</option>\r\n      <option value=\"pop\">Pop</option>\r\n    </select><br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"categoriaEstabelecimento\" class=\"label\">Categoria do Estabelecimento:</label><br />\r\n    <select v-model=\"categoriaEstabelecimento\" id=\"categoriaEstabelecimento\" class=\"input\" name=\"categoriaEstabelecimento\" required>\r\n      <option value=\"bar\">Bar</option>\r\n      <option value=\"lanchonete\">Lanchonete</option>\r\n      <option value=\"pizzaria\">Pizzaria</option>\r\n      <option value=\"hamburgueria\">Hamburgueria</option>\r\n    </select><br /><br />\r\n  </div>\r\n</div> <!-- Fechar \"Seção 3\" -->\r\n\r\n      <!--4-Prato Principal-->\r\n      <div v-if=\"currentStep === 4\">\r\n  <h2>Cadastre até 3 pratos principais</h2>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"nomePrato1\" class=\"label\">Item 1:</label><br />\r\n    <input type=\"text\" v-model=\"pratos[0].nome\" id=\"nomePrato1\" class=\"input\" name=\"nomePrato1\" /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"descricaoItem1\" class=\"label\">Descrição do item 1:</label><br />\r\n    <textarea v-model=\"pratos[0].descricao\" id=\"descricaoItem1\" class=\"input\" name=\"descricaoItem1\" rows=\"4\" maxlength=\"200\" required></textarea>\r\n    <br />\r\n    <span id=\"contador\">{{ pratos[0].descricao.length }}</span>/<span>200</span><br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"nomePrato2\" class=\"label\">Item 2:</label><br />\r\n    <input type=\"text\" v-model=\"pratos[1].nome\" id=\"nomePrato2\" class=\"input\" name=\"nomePrato2\" /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"descricaoItem2\" class=\"label\">Descrição do item 2:</label><br />\r\n    <textarea v-model=\"pratos[1].descricao\" id=\"descricaoItem2\" class=\"input\" name=\"descricaoItem2\" rows=\"4\" maxlength=\"200\" required></textarea>\r\n    <br />\r\n    <span id=\"contador\">{{ pratos[1].descricao.length }}</span>/<span>200</span><br /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"nomePrato3\" class=\"label\">Item 3:</label><br />\r\n    <input type=\"text\" v-model=\"pratos[2].nome\" id=\"nomePrato3\" class=\"input\" name=\"nomePrato3\" /><br />\r\n  </div>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"descricaoItem3\" class=\"label\">Descrição do item 3:</label><br />\r\n    <textarea v-model=\"pratos[2].descricao\" id=\"descricaoItem3\" class=\"input\" name=\"descricaoItem3\" rows=\"4\" maxlength=\"200\" required></textarea>\r\n    <br />\r\n    <span id=\"contador\">{{ pratos[2].descricao.length }}</span>/<span>200</span><br /><br />\r\n  </div>\r\n\r\n  <!-- foto prato principal -->\r\n  <div class=\"group\">\r\n    <label for=\"imagens\" class=\"label\">Imagens (Max. 3):</label><br />\r\n    <input\r\n      type=\"file\"\r\n      id=\"imagens\"\r\n      name=\"imagens[]\"\r\n      accept=\"image/jpeg, image/gif, image/png, image/tiff, image/bmp\"\r\n      multiple\r\n    /><br /><br />\r\n\r\n    <div id=\"miniatura-container\">\r\n      <div\r\n        v-for=\"(imagem, index) in imagensSelecionadas\"\r\n        :key=\"index\"\r\n        class=\"miniatura\"\r\n      >\r\n        <img :src=\"imagem.src\" alt=\"Imagem\" />\r\n        <button class=\"excluir\" @click=\"excluirMiniatura(index)\">\r\n          <i class=\"uil uil-times\"></i>\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <br /><br />\r\n  </div>\r\n</div> <!-- Fechar \"Seção 4\" -->\r\n\r\n\r\n\r\n     <!-- 5 - Foto Estabelecimento -->\r\n     <div v-if=\"currentStep === 5\">\r\n  <h2>Seção 5: Foto do Estabelecimento</h2>\r\n\r\n  <div class=\"group\">\r\n    <label for=\"estabelecimentoImagens\" class=\"label\">Imagens do Estabelecimento (Max. 3):</label><br />\r\n    <input\r\n      type=\"file\"\r\n      id=\"estabelecimentoImagens\"\r\n      name=\"estabelecimentoImagens[]\"\r\n      accept=\"image/jpeg, image/gif, image/png, image/tiff, image/bmp\"\r\n      multiple\r\n    /><br /><br />\r\n\r\n    <div id=\"miniatura-estabelecimento-container\">\r\n      <div\r\n        v-for=\"(imagem, index) in imagensEstabelecimentoSelecionadas\"\r\n        :key=\"index\"\r\n        class=\"miniatura\"\r\n      >\r\n        <img :src=\"imagem.src\" alt=\"Imagem\" />\r\n        <button class=\"excluir\" @click=\"excluirEstabelecimentoMiniatura(index)\">\r\n          <i class=\"uil uil-times\"></i>\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div> <!-- Fechar \"Seção 5\" -->\r\n\r\n\r\n  <div class=\"group\">\r\n        <div>\r\n          <input type=\"button\" class=\"button\" value=\"Salvar\" @click=\"saveForm\" />\r\n          <input type=\"button\" class=\"button\" value=\"Cancelar\" @click=\"cancelForm\" />\r\n        </div>\r\n      </div>\r\n  </div> <!-- Fechar \"posicaoform-wrap\" -->\r\n  </div> <!-- Fechar \"form-wrap\" -->\r\n</template>\r\n\r\n<script>\r\nimport IMask from \"imask\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {},\r\n  name: \"AddEstabelecimento\",\r\n  data() {\r\n    return {\r\n      imagensSelecionadas: [],\r\n      imagensEstabelecimentoSelecionadas: [],\r\n      cnpj: \"\",\r\n      cep: \"\",\r\n      endereco: {\r\n        rua: \"\",\r\n        bairro: \"\",\r\n        cidade: \"\",\r\n        uf: \"\",\r\n        cnpjInput: null,\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    excluirMiniatura(index) {\r\n      this.imagensSelecionadas.splice(index, 1);\r\n    },\r\n    cancelar() {\r\n      this.$router.push(\"/form-wrap\");\r\n    },\r\n    limpa_formulário_cep() {\r\n      document.getElementById(\"rua\").value = \"\";\r\n      document.getElementById(\"bairro\").value = \"\";\r\n      document.getElementById(\"cidade\").value = \"\";\r\n      document.getElementById(\"uf\").value = \"\";\r\n    },\r\n    meu_callback(conteudo) {\r\n      if (!(\"erro\" in conteudo)) {\r\n        document.getElementById(\"rua\").value = conteudo.logradouro;\r\n        document.getElementById(\"bairro\").value = conteudo.bairro;\r\n        document.getElementById(\"cidade\").value = conteudo.localidade;\r\n        document.getElementById(\"uf\").value = conteudo.uf;\r\n      } else {\r\n        this.limpa_formulário_cep();\r\n        alert(\"CEP não encontrado.\");\r\n      }\r\n    },\r\n    pesquisarCep() {\r\n      var cep = this.cep.replace(/\\D/g, \"\");\r\n      if (cep !== \"\") {\r\n        var validacep = /^[0-9]{8}$/;\r\n        if (validacep.test(cep)) {\r\n          axios\r\n            .get(`https://viacep.com.br/ws/${cep}/json/`)\r\n            .then((response) => {\r\n              this.meu_callback(response.data);\r\n              this.$refs.numeroInput.focus();\r\n            })\r\n            .catch((error) => {\r\n              this.limpa_formulário_cep();\r\n              alert(\"CEP não encontrado.\");\r\n            });\r\n        } else {\r\n          this.limpa_formulário_cep();\r\n          alert(\"Formato de CEP inválido.\");\r\n        }\r\n      } else {\r\n        this.limpa_formulário_cep();\r\n      }\r\n    },\r\n    excluirEstabelecimentoMiniatura(index) {\r\n    this.imagensEstabelecimentoSelecionadas.splice(index, 1);\r\n     },\r\n  },\r\n  mounted() {\r\n    this.cnpjInput = this.$refs.cnpjInput; // Add ref=\"cnpjInput\" to the input element\r\n  const cnpjMask = IMask(this.cnpjInput, {\r\n    mask: \"00.000.000/0000-00\",\r\n  });\r\n\r\n    const telefoneInput = document.getElementById(\"telefone\");\r\n  const telefoneMask = IMask(telefoneInput, {\r\n    mask: \"(00) 0000-0000\",\r\n  });\r\n\r\n  const celularInput = document.getElementById(\"celular\");\r\n  const celularMask = IMask(celularInput, {\r\n    mask: \"(00) 00000-0000\",\r\n  });\r\n\r\n\r\n    /*Img Cardápio*/\r\n    const imagensInput = document.getElementById(\"imagens\");\r\n\r\n    imagensInput.addEventListener(\"change\", () => {\r\n      const files = Array.from(imagensInput.files);\r\n      const imagensRestantes = 3 - this.imagensSelecionadas.length;\r\n      const novasImagens = files.slice(0, imagensRestantes);\r\n\r\n      novasImagens.forEach((file) => {\r\n        const reader = new FileReader();\r\n\r\n        reader.onload = () => {\r\n          const miniatura = { src: reader.result };\r\n          this.imagensSelecionadas.push(miniatura);\r\n        };\r\n\r\n        reader.readAsDataURL(file);\r\n      });\r\n\r\n      imagensInput.value = \"\";\r\n    });\r\n\r\n    /*Img Logo Estabelecimento*/\r\n    const estabelecimentoImagensInput = document.getElementById(\"estabelecimentoImagens\");\r\n\r\n  estabelecimentoImagensI